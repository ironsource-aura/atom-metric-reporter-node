{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "./"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_|node_modules"
  },
  "plugins": [
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "template": "templates/default",
    "recurse": true,
    "readme": "README.md"
  }
}